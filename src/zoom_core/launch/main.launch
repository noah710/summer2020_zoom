<launch>

    <master auto="start"/>

    <!-- launch arduino bridge -->
    <node name="arduino_bridge" pkg="rosserial_python" type="serial_node.py">
        <param name="port" value="/dev/ttyACM0" />
    </node>

    <node name="tf_broadcaster" pkg="zoom_core" type="tf_broadcaster"/>
    
    <include file="$(find zoom_perception)/launch/camera.launch"/>
    <include file="$(find zoom_core)/launch/localization.launch"/>
    <include file="$(find zoom_core)/launch/nav.launch"/>

</launch>
